<Window x:Class="ExcelWork.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExcelWork"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="200">
    <Grid>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Height="35" Margin="50,25,0,0" VerticalAlignment="Top" Width="100" Click="Button_Click"/>
        <Button x:Name="button_Copy" Content="Button" HorizontalAlignment="Left" Height="35" Margin="50,66,0,0" VerticalAlignment="Top" Width="100" Click="Button_Copy_Click"/>
        <Button x:Name="button_Copy1" Content="Button" HorizontalAlignment="Left" Height="35" Margin="50,107,0,0" VerticalAlignment="Top" Width="100" Click="Button_Copy1_Click"/>
    </Grid>
</Window>
